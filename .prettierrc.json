{
  "trailingComma": "all",
  "tabWidth": 2,
  "semi": true,
  "bracketSpacing": true,
  "singleQuote": true,
  "printWidth": 80,
  "proseWrap": "always",
  "endOfLine": "lf",
  "overrides": [
    {
      "files": "*.js",
      "options": {
        "tabWidth": 4,
        "useTabs": false
      }
    }
  ]
}
